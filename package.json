{
  "name": "fauna-gql-upload",
  "version": "1.5.1",
  "description": "Upload a GraphQL schema to FaunaDB using the command line.",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "bin": {
    "fauna-gql": "dist/index.js"
  },
  "scripts": {
    "test-example": "cd ./example && ts-node-script ../src/index.ts",
    "test": "jest",
    "build": "tsc",
    "lint": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Plazide/fauna-gql-upload.git"
  },
  "bugs": {
    "url": "https://github.com/Plazide/fauna-gql-upload/issues"
  },
  "homepage": "https://github.com/Plazide/fauna-gql-upload#readme",
  "directories": {
    "lib": "src/lib"
  },
  "keywords": [
    "faunadb",
    "graphql",
    "cli"
  ],
  "author": "Carl Hall√©n Jansson",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "cli-spinner": "^0.2.10",
    "dotenv": "^8.2.0",
    "eslint-plugin-jest": "^23.18.0",
    "faunadb": "^2.14.2",
    "node-fetch": "^2.6.0",
    "prompts": "^2.3.2",
    "yargs": "^15.4.0"
